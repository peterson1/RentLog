<UserControl x:Class="RentLog.ImportBYF.Remediations.VerifyLeaseMemos.DtoCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RentLog.ImportBYF.Remediations.VerifyLeaseMemos"
             xmlns:labl="clr-namespace:CommonTools.Lib45.TextLabels;assembly=CommonTools.Lib45"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <labl:DualText Text1="BillCode"   Text2="{Binding BillCode    }" />
        <labl:DualText Text1="LeaseId"    Text2="{Binding LeaseId     }" />
        <labl:DualText Text1="amt offset" Text2="{Binding AmountOffset}" />
        <labl:DualText Text1="Reason"     Text2="{Binding Reason      }" />
        <labl:DualText Text1="doc ref"    Text2="{Binding DocumentRef }" />
    </StackPanel>
</UserControl>
