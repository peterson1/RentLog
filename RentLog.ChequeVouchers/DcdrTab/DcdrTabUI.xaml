<UserControl x:Class="RentLog.ChequeVouchers.DcdrTab.DcdrTabUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:rows="clr-namespace:RentLog.ChequeVouchers.DcdrTab.PassbookRows"
             xmlns:local="clr-namespace:RentLog.ChequeVouchers.DcdrTab"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <rows:PassbookRowsUI x:Name="rows" 
                             DataContext="{Binding PassbookRows}" />
    </Grid>
</UserControl>
