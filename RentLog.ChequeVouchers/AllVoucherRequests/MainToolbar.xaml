<UserControl x:Class="RentLog.ChequeVouchers.AllVoucherRequests.MainToolbar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RentLog.ChequeVouchers.AllVoucherRequests"
             xmlns:mat="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:butn="clr-namespace:CommonTools.Lib45.StyledButtons;assembly=CommonTools.Lib45"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <mat:ColorZone Style="{StaticResource zone1}">
        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Right">

            <Button Content="{mat:PackIcon FileExcel, Size=21}"
                    Foreground="White"
                    ToolTip="Export to Excel"
                    Style="{StaticResource MaterialDesignFlatButton}"
                    Padding="3 0 3 0"
                    x:Name="btn" />

        </StackPanel>
    </mat:ColorZone>
</UserControl>
