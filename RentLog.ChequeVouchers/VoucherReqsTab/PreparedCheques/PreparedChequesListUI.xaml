<UserControl x:Class="RentLog.ChequeVouchers.VoucherReqsTab.PreparedCheques.PreparedChequesListUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RentLog.ChequeVouchers.VoucherReqsTab.PreparedCheques"
             xmlns:common="clr-namespace:RentLog.ChequeVouchers.CommonControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <common:LightColorZone x:Name="cZone" />
            <local:PreparedChequesFilterUI DataContext="{Binding Filter}" />
        </StackPanel>

        <local:PreparedChequesTable x:Name="tbl" />
    </DockPanel>
</UserControl>
